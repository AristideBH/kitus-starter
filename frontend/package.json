		"@tailwindcss/typography": "^0.5.14",
		"@types/cookie": "^0.6.0",
		"@types/eslint": "^9.6.0",
		"@types/jsonwebtoken": "^9.0.6",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.21.13",
		"clsx": "^2.1.1",
